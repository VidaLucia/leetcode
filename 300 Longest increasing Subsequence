class Solution(object):
    def lengthOfLIS(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        n=len(nums)
        ans = [1] * n
        for i in range(n):
            for j in range(i):
                if nums[j] < nums[i]:
                    ans[i] = max(ans[i], ans[j] + 1)

        return max(ans)

        # This is a max between pattern