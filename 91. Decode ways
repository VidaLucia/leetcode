class Solution(object):
    def numDecodings(self, s):
        """
        :type s: str
        :rtype: int
        """
        # we start at n = 0 iterate through s
        # we can then look one value to the right and if that value with this value does not summate to > 26 it can be decoded in that way 
        # if s[n] =0 then we skip it as you cannot decode it in any way
        if s[0]=='0':
            return 0
        n = len(s)
        dp = [0]*(n+1)
        dp[0], dp[1] = 1,1
        for i in range(2,n+1):
            one = int(s[i-1])
            two = int(s[i-2:i])
            if 1 <= one <=9:
                dp[i]+=dp[i-1]
            if 10 <= two <=26:
                dp[i] +=dp[i-2]
        return dp[n]

        